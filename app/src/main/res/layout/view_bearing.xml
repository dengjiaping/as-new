<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">


    <ScrollView
        android:id="@+id/personal_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/view_bearing_tc"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:background="@drawable/ripple_with_color">

                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="14dp"
                    android:layout_marginLeft="22dp"
                    android:layout_marginRight="22dp"
                    android:layout_marginTop="14dp"
                    android:drawableRight="@drawable/icon_next"
                    android:gravity="center_vertical"
                    android:text="胎次"
                    android:textColor="@color/colorGray3"
                    android:textSize="15dp"/>

                <TextView
                    android:id="@+id/view_bearing_tv_tc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="50dp"
                    android:textColor="#343434"
                    android:textSize="15dp"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#22000000"
                />

            <RelativeLayout
                android:id="@+id/view_bearing_fmfs"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ripple_with_color">

                <TextView
                    android:layout_width="match_parent"

                    android:layout_height="match_parent"
                    android:layout_marginBottom="14dp"
                    android:layout_marginLeft="22dp"
                    android:layout_marginRight="22dp"
                    android:layout_marginTop="14dp"
                    android:drawableRight="@drawable/icon_next"
                    android:gravity="center_vertical"
                    android:text="分娩方式"
                    android:textColor="@color/colorGray3"
                    android:textSize="15dp"/>

                <TextView
                    android:id="@+id/view_bearing_tv_fmfs"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="50dp"
                    android:textColor="#343434"
                    android:textSize="15dp"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#22000000"
                />

            <RelativeLayout
                android:id="@+id/view_bearing_fmtz"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ripple_with_color">

                <TextView
                    android:layout_width="match_parent"

                    android:layout_height="match_parent"
                    android:layout_marginBottom="14dp"
                    android:layout_marginLeft="22dp"
                    android:layout_marginRight="22dp"
                    android:layout_marginTop="14dp"
                    android:drawableRight="@drawable/icon_next"
                    android:gravity="center_vertical"
                    android:text="分娩时体重"
                    android:textColor="@color/colorGray3"
                    android:textSize="15dp"/>

                <TextView
                    android:id="@+id/view_bearing_tv_fmtz"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="50dp"
                    android:textColor="#343434"
                    android:textSize="15dp"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#22000000"
                />

            <RelativeLayout
                android:id="@+id/view_bearing_scsc"


                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ripple_with_color">

                <TextView
                    android:layout_width="match_parent"

                    android:layout_height="match_parent"
                    android:layout_marginBottom="14dp"
                    android:layout_marginLeft="22dp"
                    android:layout_marginRight="22dp"
                    android:layout_marginTop="14dp"
                    android:drawableRight="@drawable/icon_next"
                    android:gravity="center_vertical"
                    android:text="生产时长"
                    android:textColor="@color/colorGray3"
                    android:textSize="15dp"/>

                <TextView
                    android:id="@+id/view_bearing_tv_scsc"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="50dp"
                    android:textColor="#343434"
                    android:textSize="15dp"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#22000000"
                />

            <RelativeLayout
                android:id="@+id/view_bearing_fmss"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ripple_with_color">

                <TextView
                    android:layout_width="match_parent"

                    android:layout_height="match_parent"
                    android:layout_marginBottom="14dp"
                    android:layout_marginLeft="22dp"
                    android:layout_marginRight="22dp"
                    android:layout_marginTop="14dp"
                    android:drawableRight="@drawable/icon_next"
                    android:gravity="center_vertical"
                    android:text="外阴损伤"
                    android:textColor="@color/colorGray3"
                    android:textSize="15dp"/>

                <TextView
                    android:id="@+id/view_bearing_tv_fmss"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="50dp"
                    android:textColor="#343434"
                    android:textSize="15dp"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#22000000"
                />

            <RelativeLayout
                android:id="@+id/view_bearing_xstz"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ripple_with_color">

                <TextView
                    android:layout_width="match_parent"

                    android:layout_height="match_parent"
                    android:layout_marginBottom="14dp"
                    android:layout_marginLeft="22dp"
                    android:layout_marginRight="22dp"
                    android:layout_marginTop="14dp"
                    android:drawableRight="@drawable/icon_next"
                    android:gravity="center_vertical"
                    android:text="新生儿体重"
                    android:textColor="@color/colorGray3"
                    android:textSize="15dp"/>

                <TextView
                    android:id="@+id/view_bearing_tv_xstz"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="50dp"
                    android:textColor="#343434"
                    android:textSize="15dp"/>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#22000000"
                />

        </LinearLayout>
    </ScrollView>
</LinearLayout>